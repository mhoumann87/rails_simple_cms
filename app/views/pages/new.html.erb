<% @page_title = "Create Page" %>
<%= link_to("<< Back to List", pages_path, :class => "back-line") %>
<div class="pages new">
  <h2>Create Page</h2>
  <%= form_for(@page) do |f| %>
    
    <%= render(:partial => "form", :locals => { :f => f }) %>
    
    <div class="form-buttons">
      <%= f.submit("Create Page") %>
    </div>
  <% end %>
</div>
